import! "operator"
$ gt/bool A/int B/int ? "operator.gt"
$ sub/int A/int B/int ? "operator.lt"
$ add/int A/int B/int ? "operator.add"
$ print/void S/str ?

/* CompareInt compares two integers together:
** Returns  1 if A >  B
** Returns -1 if B >  A
** Returns  0 if A == B
*/
$ CompareInt/int A/int B/int (
   ? gt! A B (>  1)
  :? gt! B A (> -1)
  >0
)

